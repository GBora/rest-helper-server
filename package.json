{
  "name": "rest-helper-server",
  "version": "1.0.1",
  "description": "A server written in node to help with learning front end frameworks.",
  "main": "src/server.js",
  "scripts": {
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "watch-node": "nodemon dist/server.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "start": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GBora/rest-helper-server.git"
  },
  "keywords": [
    "REST",
    "Node"
  ],
  "author": "George Bora <gpbora@Gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GBora/rest-helper-server/issues"
  },
  "homepage": "https://github.com/GBora/rest-helper-server#readme",
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/sqlite3": "^3.1.5",
    "@types/swagger-jsdoc": "0.0.2",
    "@types/swagger-ui-express": "^3.0.0",
    "body-parser": "^1.19.0",
    "dotenv": "^7.0.0",
    "express": "^4.17.3",
    "knex": "^0.19.5",
    "sqlite3": "^4.0.6",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "concurrently": "^4.1.0",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.4"
  }
}
